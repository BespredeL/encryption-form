{
    "name": "bespredel/encryption-form",
    "description": "A Laravel package to securely encrypt form fields on the client-side using public key encryption and decrypt them on the server-side using the private key.",
    "keywords": [
        "laravel",
        "encryption",
        "security",
        "data-privacy",
        "rsa-encryption",
        "privacy-protection",
        "client-side-encryption",
        "form-encryption",
        "openssl"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Kireev",
            "email": "hello@bespredel.name",
            "homepage": "https://bespredel.name",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-openssl": "*",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Bespredel\\EncryptionForm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bespredel\\EncryptionForm\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bespredel\\EncryptionForm\\EncryptionFormServiceProvider"
            ]
        }
    }
}
